{
  "name": "shot-put",
  "version": "0.10.1",
  "description": "CLI tool to watch desktop for files of a given extension and automatically move them to a given directory",
  "main": "index.js",
  "scripts": {
    "test": "ava --verbose",
    "watch": "npm t -- -w"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "auto",
    "automate",
    "automatically",
    "automation",
    "cli",
    "cleanup",
    "desktop",
    "dir",
    "directory",
    "fs",
    "file-system",
    "osx",
    "screencap",
    "screenshot",
    "transfer"
  ],
  "author": "Kevin Donahue <kevindonahuedevelopment@gmail.com> (https://twitter.com/recur_excur)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevmannn/shot-put"
  },
  "devDependencies": {
    "ava": "^0.14.0"
  },
  "dependencies": {
    "ansi-escapes": "^1.4.0",
    "async": "^2.0.0-rc.3",
    "chalk-form": "^0.1.2",
    "chokidar": "^1.6.0",
    "lodash": "^4.13.1",
    "meow": "^3.7.0",
    "mkdirp": "^0.5.1",
    "os-homedir": "^1.0.1",
    "path-exists": "^2.1.0",
    "pify": "^2.3.0",
    "pump": "^1.0.1",
    "rimraf": "^2.5.2",
    "single-line-log": "^1.1.1",
    "untildify": "^2.1.0"
  }
}
