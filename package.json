{
  "name": "shot-put",
  "version": "0.10.13",
  "description": "CLI tool to watch desktop for files of a given extension and automatically move them to a given directory",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "test:watch": "npm t -- -w"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "auto",
    "automate",
    "automatically",
    "automation",
    "cli",
    "cleanup",
    "desktop",
    "dir",
    "directory",
    "fs",
    "file-system",
    "macOS",
    "osx",
    "screencap",
    "screenshot",
    "transfer"
  ],
  "author": "Kevin Donahue <kevindonahuedevelopment@gmail.com> (https://twitter.com/nonnontrivial)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevmannn/shot-put"
  },
  "devDependencies": {
    "ava": "^0.19.1"
  },
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "chalk-form": "^1.0.0",
    "chokidar": "^1.6.0",
    "graceful-fs": "^4.1.4",
    "lodash": "^4.14.0",
    "meow": "^3.7.0",
    "mkdirp": "^0.5.1",
    "multistream": "^2.0.5",
    "ora": "^1.2.0",
    "os-homedir": "^1.0.1",
    "path-exists": "^3.0.0",
    "pify": "^2.3.0",
    "pump": "^1.0.1",
    "rimraf": "^2.5.2",
    "single-line-log": "^1.1.1",
    "untildify": "^3.0.2"
  }
}
